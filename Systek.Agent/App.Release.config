<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using app.config transformation visit http://go.microsoft.com/fwlink/?LinkId=125889 -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="Port" value="##SYSTEK_SERVER_PORT##" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="ServerIP" value="##SYSTEK_SERVER_IP##" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="LocalLogPath" value="##SYSTEK_LOG_FILEPATH##" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="ReconnectWait" value="5000" xdt:Transform="Replace" xdt:Locator="Match(key)" />
  </appSettings>
</configuration>